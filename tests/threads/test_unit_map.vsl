
FUNC VOID printTabs(tab[], save[], size)
{
    INT cmp
    cmp := size
    WHILE cmp DO {
        PRINT save[size - cmp], "^2 = ", tab[size - cmp], "\n"
        cmp := cmp - 1
    } DONE
}

ROUTINE VOID routine(a[], b)
{
    WHILE b DO {
        b := b-1
        a[b] := a[b] * a[b]
    } DONE
}

FUNC VOID main()
{
    INT a, t[10], save[10]

    // Initialization of the tab t
    a := 10
    WHILE a DO {
        t[10 - a] := 10 - a
        save[10 - a] := 10 - a
        a := a - 1
    } DONE

    MAP t, 2, 10, routine

    printTabs(t, save, 10)
}