
FUNC INT f(a)
{
    RETURN a*a
}

ROUTINE VOID routine(a[], b)
{
    WHILE b DO {
        b := b-1
        a[b] := a[b] * a[b]
    } DONE
}

FUNC VOID main()
{
    INT a, t[30]
    a := 30
    WHILE a DO {
        a := a - 1
        t[29 - a] := 29 - a
        PRINT "t[", 29 - a, "]\t=\t", t[29 - a], "\n"
    } DONE
    MAP t, 2, 30, f
    a := 30
    WHILE a DO {
        a := a - 1
        PRINT "t[", 29 - a, "]\t=\t", t[29 - a], "\n"
    } DONE
}