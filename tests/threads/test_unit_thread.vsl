FUNC VOID jackie1()
{
    INT a, b[1000000]
    a := 1000000
    WHILE a DO {
        a := a - 1
        b[a] := a
    } DONE
}

FUNC VOID jackie2()
{
    INT a, b[1000000]
    a := 1000000
    WHILE a DO {
        a := a - 1
        b[a] := a
    } DONE
}

FUNC INT jackie3(a, b[])
{
    WHILE a DO {
        a := a - 1
        b[a] := 2 * a
    } DONE
    PRINT b[950000], "\n"
    RETURN b[950000]
}

FUNC VOID main()
{
    TID a, b, c
    INT t[1000000], z
    THREAD a, jackie1
    THREAD b, jackie2
    THREAD c, jackie3, 1000000, t
    JOIN a
    JOIN b
    JOIN c, z
    PRINT z, "\n"
    PRINT t[950000], "\n"
}